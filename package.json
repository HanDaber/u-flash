{
  "name": "base",
  "version": "1.0.0",
  "description": "base dev app",
  "main": "index.js",
  "engine": "node >= 5.10.x",
  "config": {
    "buildDir": "./public",
    "devServerEndpoint": "http://localhost:8080/",
    "devBackendEndpoint": "http://localhost:3000/",
    "serverEndpoint": "http://localhost:8080/",
    "backendEndpoint": "http://localhost:3000/",
    "serverBasePath": "/base",
    "devHost": "localhost",
    "devPort": 8000,
    "serverHost": "localhost",
    "serverPort": 8080,
    "dataHost": "localhost",
    "dataPort": 3000
  },
  "scripts": {
    "preinstall": "echo INSTALLING APP NOW READY GO SUPER POWERS START!\n\n\n",
    "build": "NODE_ENV=production webpack --colors --progress --config ./webpack/config",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "sudo npm install",
    "start": "sudo NODE_ENV=production forever start server.js >> server.log",
    "dev": "NODE_ENV=development nodemon dev-server ./webpack/config",
    "webpack": "webpack --colors --progress --config --display-modules --display-reasons ./webpack/config"
  },
  "author": "Dan Haber",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-proxy": "^1.0.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "document-register-element": "^0.5.4",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "glob": "^7.0.3",
    "html-loader": "^0.4.3",
    "ignore-loader": "^0.1.1",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "json-loader": "^0.5.4",
    "jstransformer-babel": "^1.0.1",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "net": "^1.0.2",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "opn": "^4.0.1",
    "phantomjs": "^2.1.7",
    "postcss-loader": "^0.8.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "template-html-loader": "0.0.3",
    "tls": "0.0.1",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  }
}
